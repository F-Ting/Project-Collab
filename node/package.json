{
  "name": "node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "frontend": "npm start --prefix ../angular",
    "microservice": "npm start --prefix ../node2",
    "dev": "concurrently \"npm run start\" \"npm run frontend\" \"npm run microservice\"",
    "db:clean": "npm run db:drop && npm run db:create && npm run db:migrate",
    "db:create": "babel-node ./db/create",
    "db:drop": "babel-node ./db/drop",
    "db:migrate": "babel-node ./db/migrate",
    "db:reset": "sequelize db:migrate:undo:all && sequelize db:migrate && psql -U postgres -d project-collab-db -f db/handcraft_data.sql ",
    "pretest": "set NODE_ENV=test&& sequelize db:migrate",
    "test": "set NODE_ENV=test&& mocha",
    "test:report": "nyc report --reporter=text-summary",
    "setup_test": "psql -U postgres < db/setup.sql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
